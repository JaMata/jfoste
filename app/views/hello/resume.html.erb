<div class="container-fluid container-buffer">
	<div id="resume-carousel" class="carousel jordi slide col-sm-offset-2 col-md-offset-2 col-lg-offset-2 col-xs-12 col-sm-8 col-md-8 col-lg-8" data-ride="carousel" data-interval="false">
	  <!-- Nav Circles -->
		<ol class="carousel-indicators jordi">
	    	<li data-target="#resume-carousel" data-slide-to="0" class="active"></li>
	    	<li data-target="#resume-carousel" data-slide-to="1"></li>
	    	<li data-target="#resume-carousel" data-slide-to="2"></li>
	  	</ol>

	  <!-- Wrapper for slides -->
	    <div class="row">
		  	<div class="carousel-inner jordi" role="listbox">
		  		<!-- BEGIN SLIDE 1 EDUCATION -->
		    	<div class="item active col-xs-offset-1 col-sm-offset-2 col-md-offset-2 col-lg-offset-2 col-xs-10 col-sm-8 col-md-8 col-lg-8">
			    	<div class="title-text">Education</div>
					<hr class="lime-border" />
			    	<div class="row row-buffer lime-outline">
			    		<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
			    			<%= image_tag 'towsonu-logo.png', alt: 'Towson University', class: 'img-md' %>
			    		</div>
						<div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
							<div class="sub-text">August 2011 - May 2015</div>
							<div class="item-text">Towson University</div>
							<div class="item-text">Bachelor of Science, Bioinformatics</div>
							<div class="item-text">Computer Science minor</div>
							<div class="sub-text"><em>cum laude</em></div>
						</div>
					</div>	
			    </div>
			    <!-- BEGIN SLIDE 2 EXPERIENCE -->
			    <div class="item col-xs-offset-1 col-sm-offset-2 col-md-offset-2 col-lg-offset-2 col-xs-10 col-sm-8 col-md-8 col-lg-8">
			    	<div class="title-text">Experience</div>
			    	<hr class="lime-border" />
			    	<div class="row row-buffer lime-outline">
			    		<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
			    			<%= image_tag 'epa-logo.png', alt: 'US EPA', class: 'img-md' %>
			    		</div>
						<div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
							<div class="sub-text">October 2015 - September 2016</div>
							<div class="item-text">Application and Database Developer, Durham, NC</div>
							<div class="item-text">Oak Ridge Associated Universities / US EPA</div>
							<div class="sub-text">Created Rails applications that provided interfaces to chemical databases.</div>
						</div>
					</div>
					<div class="row row-buffer lime-outline">
			    		<div class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
			    			<%= image_tag 'towsonu-logo.png', alt: 'Towson University', class: 'img-md' %>
			    		</div>
						<div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
							<div class="sub-text">August 2013 - May 2014</div>
							<div class="item-text">Resident Assistant, Towson, MD</div>
							<div class="item-text">Housing and Residence Life</div>
							<div class="sub-text">Provided residents with a safe and enjoyable campus life.</div>
						</div>
					</div>	
			    </div>
			    <!-- BEGIN SLIDE 3 TECHNOLOGIES -->
			    <div class="item col-xs-offset-1 col-sm-offset-2 col-md-offset-2 col-lg-offset-2 col-xs-10 col-sm-8 col-md-8 col-lg-8">
			    	<div class="title-text">Technologies</div>
			    	<hr class="lime-border" />
			    	<div class="row row-buffer">
			    		<!-- TODO: Store the titles and file names in model, controller, db, or tsv and loop -->
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'html-logo.png', alt: 'HTML5', class: 'img-sm lime-outline', title: 'HTML5' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'css-logo.png', alt: 'CSS3', class: 'img-sm lime-outline', title: 'CSS3' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'javascript-logo.png', alt: 'JavaScript', class: 'img-sm lime-outline', title: 'JavaScript' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'rails-logo.png', alt: 'Ruby on Rails', class: 'img-sm lime-outline', title: 'Ruby on Rails' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'bootstrap-logo.png', alt: 'Bootstrap', class: 'img-sm lime-outline', title: 'Bootstrap' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'd3-logo.png', alt: 'D3.js', class: 'img-sm lime-outline', title: 'D3.js' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'java-logo.png', alt: 'Java', class: 'img-sm lime-outline', title: 'Java' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'cplus-logo.png', alt: 'C++', class: 'img-sm lime-outline', title: 'C++' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'python-logo.png', alt: 'Python', class: 'img-sm lime-outline', title: 'Python' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'mysql-logo.png', alt: 'MySql', class: 'img-sm lime-outline', title: 'MySql' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'git-logo.png', alt: 'Git', class: 'img-sm lime-outline', title: 'Git' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'atlassian-logo.png', alt: 'Atlassian Suite', class: 'img-sm lime-outline', title: 'Atlassian Suite' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'command-logo.png', alt: 'Command Line', class: 'img-sm lime-outline', title: 'Command Line' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'linux-logo.png', alt: 'Linux Environments', class: 'img-sm lime-outline', title: 'Linux Environments' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'ms-logo.png', alt: 'Microsoft Office', class: 'img-sm lime-outline', title: 'Microsoft Office' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'mybalsamiq-logo.png', alt: 'MyBalsamiq', class: 'img-sm lime-outline', title: 'MyBalsamiq' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'nik-logo.png', alt: 'Google Nik Collection', class: 'img-sm lime-outline', title: 'Google Nik Collection' %>
			    		</div>
			    		<div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
			    			<%= image_tag 'gimp-logo.png', alt: 'GIMP', class: 'img-sm lime-outline', title: 'GIMP' %>
			    		</div>
			    	</div>
			  	</div>
			</div>  	
	    </div>
	  <!-- Prev & Next Nav Arrows  -->
	  <!-- TODO: Switch to Font Awesome thinner chevrons -->
	  	<a class="left carousel-control" href="#resume-carousel" role="button" data-slide="prev">
	    	<span class="glyphicon glyphicon-chevron-left lime" aria-hidden="true"></span>
	    	<span class="sr-only">Previous</span>
	  	</a>
	  	<a class="right carousel-control" href="#resume-carousel" role="button" data-slide="next">
	    	<span class="glyphicon glyphicon-chevron-right lime" aria-hidden="true"></span>
	    	<span class="sr-only">Next</span>
	  	</a>
	</div>
</div>